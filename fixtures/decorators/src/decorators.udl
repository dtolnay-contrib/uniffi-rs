namespace decorators {};

[Decorator]
interface MyDecorator {
  Any? with_return();
  void string_saver();
  i32  with_counter();
};

[Decorator=MyDecorator]
interface RustObject {
  constructor();

  [Name=from_string]
  constructor(string string);

  [CallWith=string_saver]
  string identity_string(string s);

  [CallWith=with_counter]
  string get_string();

  [CallWith=with_return]
  i32 length();
};